{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "firstName": {
              "type": "string"
            },
            "lastName": {
              "type": "string"
            },
            "skillmap": {
              "type": "array",
              "items": {}
            }
          },
          "required": [
            "id",
            "firstName",
            "lastName",
            "skillmap"
          ]
        }
      ]
    }
  },
  "required": [
    "users"
  ]
}